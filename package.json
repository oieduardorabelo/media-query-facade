{
  "name": "media-query-facade",
  "version": "1.0.19",
  "description": "A nicer JavaScript media query API.",
  "main": "index.js",
  "homepage": "https://github.com/tanem/media-query-facade",
  "bugs": {
    "url": "http://github.com/tanem/media-query-facade/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tanem/media-query-facade.git"
  },
  "scripts": {
    "lint": "jshint ./lib/*.js ./test/*.js ./example/main.js index.js",
    "test-spec": "npm run lint && testling",
    "test-cov": "npm run lint && browserify -t coverify test/media-query-facade.test.js | testling | coverify",
    "example": "npm run lint && browserify example/main.js > example/bundle.js && open example/index.html",
    "test": "npm run test-cov"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "keywords": [
    "media",
    "query",
    "facade"
  ],
  "author": {
    "name": "Tane Morgan",
    "url": "http://github.com/tanem"
  },
  "license": "MIT",
  "devDependencies": {
    "browserify": "^11.0.1",
    "coverify": "^1.4.1",
    "jshint": "^2.8.0",
    "tape": "^4.2.0",
    "testling": "^1.7.1"
  },
  "dependencies": {
    "lodash.bind": "^3.1.0",
    "lodash.foreach": "^3.0.3",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^3.0.1",
    "lodash.merge": "^3.3.2",
    "lodash.omit": "^3.1.0",
    "lodash.some": "^3.2.3"
  },
  "testling": {
    "files": "test/media-query-facade.test.js",
    "browsers": [
      "ie/latest",
      "chrome/latest",
      "firefox/latest",
      "safari/latest",
      "opera/latest",
      "iphone/latest",
      "ipad/latest",
      "android-browser/latest"
    ]
  }
}
